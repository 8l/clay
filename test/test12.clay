import base.*;
import libc.(printf);

map(f, a) {
    static ResultType = TypeOf(f(a[0]));
    var b = Vector[ResultType]();
    for (x in a)
        push(b, f(x));
    return b;
}

main() {
    var a = Vector([1, 2, 3, 4, 5, 6]);

    var f = lambda (x) { return x*10; };
    var b = map(f, a);

    for (x in b)
        printf(cstring("%d\n"), x);

    var c = map(lambda (x) { return x*x*x; }, a);

    for (x in c)
        printf(cstring("%d\n"), x);
}
