import base.*;
import filemaps.*;

main() {
    static N = 100;
    {
        var f = File("tempfile.txt", WRITE_READ);
        printTo(f, String(Array[Char,N]()));
    }
    {
        var f = File("tempfile.txt", READ_WRITE);
        var fmap = FileMap(f, 0, N);
        for (i in range(N))
            (fmap.addr + i)^ = Byte((i % 26) + 65);
    }
}
