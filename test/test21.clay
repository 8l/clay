
record Foo {
    value:Int;
}

overload Foo() = Foo(0);

overload Foo(value:Int) : Foo {
    returned.value = value;
    println("constructed value = ", returned.value);
}

overload Foo(f:Foo) = Foo(f.value);

overload destroy(f:Foo) {
    println("destroyed value = ", f.value);
}

main() {
    var p = allocate(Foo(10));
    var q = allocate(Foo());
    println("assigning");
    q = p;
    println("assigned");
}
