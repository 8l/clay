import kernel.*;

insertionSort(a) {
    var i = begin(a) + 1;
    while (i < end(a)) {
        var x = at(i);
        var j = i;
        while (at(j-1) > x) {
            at(j) = at(j-1);
            j = j - 1;
            if (j == begin(a)) break;
        }
        at(j) = x;
        i = i + 1;
    }
}

reverse(a) {
    var n = size(a);
    for (i in range(n / 2))
        swap(at(a, i), at(a, n-i-1));
}

main() {
    var a = Vector[Int32]();
    for (i in range(1000))
        pushBack(a, i*2);
    for (i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return at(a, 0);
}
