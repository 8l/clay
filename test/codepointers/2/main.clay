
import libc;

main() {
    println(Type(libc.printf));
    info(libc.printf);
    info(libc.strcpy);
}


procedure info;

[In,Out]
overload info(x:VarArgsCCodePointer[In,Out]) {
    println();
    println("VarArgsCCodePointer");
    println("In = ", ...values(In));
    println("Out = ", ...values(Out));
}

[In,Out]
overload info(x:CCodePointer[In,Out]) {
    println();
    println("CCodePointer");
    println("In = ", ...values(In));
    println("Out = ", ...values(Out));
}

procedure values(x) = x;
[...T] overload values(x:Tuple[...T]) = ...unpack(x);
