import base;

def insertionSort(ref a) {
    var i = 1;
    while (lesser(i, size(a))) {
        var x = at(a, i);
        var j = i;
        while (greater(at(a, subtract(j, 1)), x)) {
            at(a, j) = at(a, subtract(j, 1));
            j = subtract(j, 1);
            if (equals(j, 0)) break;
        }
        at(a, j) = x;
        i = add(i, 1);
    }
}

def swap(ref a, ref b) {
    var temp = a;
    a = b;
    b = temp;
}

def reverse(ref a) {
    var n = size(a);
    for (var i in range(divide(n, 2))) {
        var j = subtract(subtract(n, i), 1);
        swap(at(a, i), at(a, j));
    }
}

def main() {
    var a = array(Int, 1000);
    var n = size(a);
    for (var i in range(n))
        at(a, i) = multiply(i, 2);
    for (var i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return at(a, 0);
}
