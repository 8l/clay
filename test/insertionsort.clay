import base;

def insertionSort(ref a) {
    var i = 1;
    while (i < size(a)) {
        var x = at(a, i);
        var j = i;
        while (at(a,j-1) > x) {
            at(a, j) = at(a, j-1);
            j = j - 1;
            if (j == 0) break;
        }
        at(a, j) = x;
        i = i + 1;
    }
}

def swap(ref a, ref b) {
    var temp = a;
    a = b;
    b = temp;
}

def reverse(ref a) {
    var n = size(a);
    for (var i in range(n / 2))
        swap(at(a, i), at(a, n-i-1));
}

def main() {
    var a = array(Int, 1000);
    var n = size(a);
    for (var i in range(n))
        at(a, i) = i*2;
    for (var i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return at(a, 0);
}
