import base.*;
import libc;

overloadable print;

overload print(x:Int32) {
    libc.printf(cstring("int value = %d\n"), x);
}

overload print(x:UInt32) {
    libc.printf(cstring("uint value = %d\n"), x);
}

overload print(x:Float64) {
    libc.printf(cstring("double value = %f\n"), x);
}

main() {
    var a = 1 + 2#u32;
    print(a);
    return 0;
}
