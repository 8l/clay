import io.streams.*;

import data.vectors.*;

import test.*;

main() = testMain(
    TestSuite("io.streams.bytevector",
        TestCase("OutputStream?", -> {
            expectTrue(OutputStream?(Vector[Char]));
        }),
        TestCase("write", -> {
            var r = Vector[Char]();
            write(r, "aabb");
            write(r, "cc");
            expectEqual("aabbcc", r);
        }),
    ));
