    println(Type(trailingZeros(0x10u)));
    println(Type(leadingZeros (0x10us)));
    println(Type(trailingZeros(0x10us)));
    println(Type(leadingZeros (0x10uss)));
    println(Type(trailingZeros(0x10uss)));
    println();
