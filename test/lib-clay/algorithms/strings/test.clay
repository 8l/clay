import test.*;
import algorithms.strings.(trim);

main() = testMain(
    TestSuite(
        "algorithms.strings", array(
            TestCase("trim", -> {
                expectEqual(
                    trim("   \t A string,\nthat\tneeds trimming. \n  \n "),
                    "A string,\nthat\tneeds trimming.",
                );
                expectEqual(
                    trim("*** message ***", x => (x == '*' or x == ' ')),
                    "message"
                );
            }),
        )
    )
);
