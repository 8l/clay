import test.*;
import data.algorithms.permutations.*;
import printer.(str);
import data.sequences.*;
import data.vectors.*;

fact(n){
  var result = 1;
  for(i in range(1, n + 1)) result *: i;
  return result;
}

baseTestCase(n) = 
  TestCase(str("Permuting 1..", n), => {
    var it = Vector[Int](range(1,n+1));

    var counter = 0;
    eachPermutation(it, -> {
      counter +: 1;
    });

    expectEqual(fact(n), counter);
  });

main() = testMain(
  TestSuite("Permutations of integer ranges", ..mapValues(baseTestCase, 1,2,3,4,5,6,7))
);
