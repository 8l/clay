
even?(x) = (x % 2 == 0);

main() {
    var h = HashMap[Int,Int]();

    for (i in range(10))
        h[i*i] = i;

    for (x in range(100)) {
        var flag = contains?(h, x);
        if (even?(x)) {
            var result = remove(h, x);
            assert(flag == result);
        }
    }

    for (k,v in items(h))
        println(k, ", ", v);

    var hh = HashMap[Int,Int]();
    hh = h;

    println();
    for (k,v in items(hh))
        println(k, ", ", v);

    hh = hh;

    println();
    for (k,v in items(hh))
        println(k, ", ", v);
}
