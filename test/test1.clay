import base;

def factorial(n) {
    if (n == 0)
        return 1;
    return n*factorial(n-1);
}

def isEven(n) {
    if (n < 2)
        return n == 0;
    return isOdd(n-1);
}

def isOdd(n) {
    if (n < 2)
        return n == 1;
    return isEven(n-1);
}

def foo() {
    return array(Int, 10);
}

record NodeA[T] {
    value: T;
    next: Pointer[NodeB[T]];
}

record NodeB[T] {
    ptr: Pointer[T];
    next: Pointer[NodeA[T]];
}

def main() {
    let n = 7;
    n = n - 1;
    let f = factorial(n);
    let p = default(NodeA[(Int,Bool)]);
    return (foo(), isEven(n), f, p);
}
