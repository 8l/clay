import math.(pow);
import operators.*;

(**)(a,b) = pow(a,b);
(***)(a,b) = pow(a,b);
(****)(a,b) = pow(a,b);

overload operatorPrecedence(#(**)) = 900;
overload operatorAssociativity(#(**)) = LEFT;

overload operatorPrecedence(#(***)) = 890;
overload operatorAssociativity(#(***)) = RIGHT;

overload operatorPrecedence(#(****)) = 880;
overload operatorAssociativity(#(****)) = RIGHT;

main() {
    
    println(4 ** 3 ** 2);
    println(4 *** 3 *** 2);
    println(4 **** 3 **** 2);

    println(4 ** 3 *** 2);
    println(4 *** 3 ** 2);
    println(4 ** 3 **** 2);
    println(4 **** 3 ** 2);
    println(4 **** 3 *** 2);
    println(4 *** 3 **** 2);

}
