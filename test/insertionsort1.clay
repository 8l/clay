import base;

def insertionSort(a) {
    let i = 1;
    while (i < size(a)) {
        let x = at(a, i);
        let j = i;
        while (at(a,j-1) > x) {
            at(a, j) = at(a, j-1);
            j = j - 1;
            if (j == 0) break;
        }
        at(a, j) = x;
        i = i + 1;
    }
}

def reverse(a) {
    let n = size(a);
    for (i in range(n / 2))
        swap(at(a, i), at(a, n-i-1));
}

def main() {
    let a = array(Int, 1000);
    for (i in range(size(a)))
        at(a, i) = i*2;
    for (i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return at(a, 0);
}
