import kernel.*;

insertionSort(a) {
    var i = 1;
    while (i < size(a)) {
        var x = item(a, i);
        var j = i;
        while (item(a,j-1) > x) {
            item(a, j) = item(a, j-1);
            j = j - 1;
            if (j == 0) break;
        }
        item(a, j) = x;
        i = i + 1;
    }
}

reverse(a) {
    var n = size(a);
    for (i in range(n / 2))
        swap(item(a, i), item(a, n-i-1));
}

main() {
    var a = Array[Int32, 1000]();
    for (i in range(size(a)))
        item(a, i) = i*2;
    for (i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return item(a, 0);
}
