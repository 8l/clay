import deques.*;

main() {
    var d1 = Deque[Int]();
    alias TEST_RANGE = 500;

    for (i in range(TEST_RANGE)) {
        pushFront(d1, 2*i);
        push(d1, 2*i + 1);
    }

    println("Full size ", size(d1));

    println("Iterating:");
    
    var n = 0;
    for (x in d1) {
        print(x, " ");
        n += 1;
        if (n == 20) {
            println();
            n = 0;
        }
    }
    println();

    println("Popping:");

    n = 0;
    for (i in range(TEST_RANGE)) {
        var x = popFront(d1);
        var y = pop(d1);

        print(y, " ", x, " ");
        n += 2;
        if (n == 20) {
            println();
            n = 0;
        }
    }

    println("Empty size ", size(d1));
}
