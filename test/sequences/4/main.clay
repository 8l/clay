
main() {
    {
        // test enumerate
        var a = Vector([10, 20, 40, 80, 160]);
        for (i, x in enumerate(a))
            println(i, ") ", x);
    }

    {
        // test map, range, reduce
        var a = map(lambda (x) { return x + 1; },
                    range(10));
        var s = reduce(add, 0, a);
        println(s);
    }
}
