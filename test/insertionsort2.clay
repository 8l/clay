import base;

def insertionSort(a) {
    let i = begin(a) + 1;
    while (i < end(a)) {
        let x = at(i);
        let j = i;
        while (at(j-1) > x) {
            at(j) = at(j-1);
            j = j - 1;
            if (j == begin(a)) break;
        }
        at(j) = x;
        i = i + 1;
    }
}

def reverse(a) {
    let n = size(a);
    for (i in range(n / 2))
        swap(at(a, i), at(a, n-i-1));
}

def main() {
    let a = array(Int32, 1000);
    for (i in range(size(a)))
        at(a, i) = i*2;
    for (i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    return at(a, 0);
}
