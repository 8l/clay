import base.*;
import libc;

main() {
    libc.printf(cstring("sizeof(Byte) = %d\n"), TypeSize(Byte));
    libc.printf(cstring("sizeof(Short) = %d\n"), TypeSize(Short));
    libc.printf(cstring("sizeof(Int) = %d\n"), TypeSize(Int));
    libc.printf(cstring("sizeof(Long) = %d\n"), TypeSize(Long));

    libc.printf(cstring("sizeof(RawPointer) = %d\n"), TypeSize(RawPointer));
    libc.printf(cstring("sizeof(SizeT) = %d\n"), TypeSize(SizeT));
    libc.printf(cstring("sizeof(PtrInt) = %d\n"), TypeSize(PtrInt));
}
