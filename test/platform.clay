import base.*;

main() {
    println("sizeof(Byte) = ", TypeSize(Byte));
    println("sizeof(Short) = ", TypeSize(Short));
    println("sizeof(Int) = ", TypeSize(Int));
    println("sizeof(Long) = ", TypeSize(Long));

    println("sizeof(RawPointer) = ", TypeSize(RawPointer));
    println("sizeof(SizeT) = ", TypeSize(SizeT));
    println("sizeof(PtrInt) = ", TypeSize(PtrInt));
}
