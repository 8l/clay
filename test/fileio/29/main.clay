import files.mapping.*;

main() {
    alias N = 500*1000;

    var file = File("tempfile.txt", CREATE);
    seek(file, N-1);
    writeByte(file, 'B');

    var mapping = FileMapping(file, MAP_READ_WRITE);
    for (i in range(N))
        (mappingAddress(mapping) + i)^ = Byte((i % 26) + 65);
    flushMapping(mapping);
}
