import base;

def factorial1(n) {
    if (n == 0)
        return 1;
    return n*factorial1(n-1);
}

def factorial2(n) {
    let p = 1;
    again :
    if (n == 0)
        return p;
    p = p * n;
    n = n - 1;
    goto again;
}

def factorial3(n) {
    let p = 1;
    while (true) {
        if (n == 0) break;
        p = p * n;
        n = n - 1;
    }
    return p;
}

def factorial4(n) {
    let p = 1;
    for (i in range(n))
        p = p * (i+1);
    return p;
}

def main() {
    let n = 7;
    n = n - 1;
    let f = factorial4(n);
    return f;
}
