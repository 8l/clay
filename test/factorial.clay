import base;

def factorial1(n) {
    if (equals(n, 0))
        return 1;
    return multiply(n, factorial(subtract(n, 1)));
}

def factorial2(n) {
    var p = 1;
    again :
    if (equals(n, 0))
        return p;
    p = multiply(p, n);
    n = subtract(n, 1);
    goto again;
}

def factorial3(n) {
    var p = 1;
    while (true) {
        if (equals(n, 0)) break;
        p = multiply(p, n);
        n = subtract(n, 1);
    }
    return p;
}

def factorial4(n) {
    var p = 1;
    for (var i in range(n))
        p = multiply(p, add(i, 1));
    return p;
}

def main() {
    var n = 7;
    n = subtract(n, 1);
    var f = factorial4(n);
    return f;
}
