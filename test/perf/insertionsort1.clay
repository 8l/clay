import base.*;
import libc;

insertionSort(a) {
    var i = 1;
    while (i < size(a)) {
        var x = a[i];
        var j = i;
        while (a[j-1] > x) {
            a[j] = a[j-1];
            j -= 1;
            if (j == 0) break;
        }
        a[j] = x;
        i += 1;
    }
}

reverse(a) {
    var n = size(a);
    for (i in range(n / 2))
        swap(a[i], a[n-i-1]);
}

main() {
    var a = Array[Int, 1000]();
    for (i in range(size(a)))
        a[i] = Int(i*2);
    for (i in range(100)) {
        insertionSort(a);
        reverse(a);
    }
    libc.printf(cstring("a[0] = %d\n"), a[0]);
    return 0;
}
