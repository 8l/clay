import kernel.*;
import libc.*;

[F,T]
map(f:F, a:Vector[T]) {
    static ResultElement = TypeOf(f(a[0]));
    var b = Vector[ResultElement]();
    for (x in a)
        push(b, f(x));
    return b;
}

[T]
scale(factor, a:Vector[T]) {
    var f = lambda (x) { return x*factor; };
    return map(f, a);
}

main() {
    var a = Vector([1, 2, 3, 4, 5, 6]);

    for (x in a)
        printf(cstring("%d\n"), x);

    var b = scale(100, a);

    for (x in b)
        printf(cstring("%d\n"), x);
}
