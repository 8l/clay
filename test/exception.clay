record Test {
    x: Int32;
}

overload destroy(t:Test) {
    println("Destroyed ", t.x);
}

m(x) {
    try {
        var r = Test(1);
        if (x == 1)
            throw();
        var r2 = Test(2);
    } catch {
        println("Caught. Throwing again.");
        throw();
    }
    var r2 = Test(3);
}

main() {
    try {
        println("Hello World");
        m(0);
        var r = Test(20);
        m(1);
        var r = Test(30);
    } catch {
        println("Caught");
    } finally {
        println("Finally");
    }
    return 0;
}
