def fact(n) {
    if (intEquals(n,0))
        return 1;
    return intMultiply(n, fact(intSubtract(n,1)));
}

def isEven(n) {
    return isOdd(intSubtract(n, 1));
}

def isOdd(n) {
    if (intGreater(n, 1))
        return isEven(intSubtract(n, 1));
    return intEquals(n, 1);
}


record NodeA[T] {
    x: T;
    next: Pointer[NodeA[T]];
}

def null[T](type T):Pointer[T] {
    return default(Pointer[T]);
}

def mainA() {
    var i = 10;
    var n = allocate(NodeA[Int]);
    n^ = NodeA(i, null(NodeA[Int]));
    return n^.next;
}

def main() {
    var a = "hello";
    return mainA();
}
