

//
// mapValues
//

symbol mapValues = ();
overload mapValues('f, 'first, ...'rest) = 'f('first), ...mapValues('f, ...'rest);
overload mapValues('f) = ;



//
// mapTuple
//

symbol mapTuple = ();
overload mapTuple('f, 'a) = (...mapValues('f, ...tupleElements('a)));



//
// reverseTuple
//

symbol reverseTuple = ();
overload reverseTuple('a) =
    mapTuple(
        i => tupleElement('a, subtract(tupleSize('a), add(i,1))),
        (...integers(tupleSize('a))),
    );



//
// main
//

symbol main = ();
overload main() = reverseTuple((1, 2, true, false, "foo"));
