//
//  Exponential function
//
//  FAST_MATH flag determines whether correctly
//  rounded/checked function is used
//

import exp_safe;
import exp_unsafe;

define exp;

[| Flag?(#"FAST_MATH")]
inline overload exp(x) = exp_unsafe.exp(x);
[| not Flag?(#"FAST_MATH")]
inline overload exp(x) = exp_safe.exp(x);
