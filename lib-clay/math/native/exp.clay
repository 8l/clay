//
//  Exponential function
//
//  FAST_MATH flag determines whether correctly
//  rounded/checked function is used
//

import math.native.exp_safe.(exp_safe);
import math.native.exp_unsafe.(exp_unsafe);

define exp;

[T | Float?(T) and Flag?(#"FAST_MATH")]
inline overload exp(x:T) = exp_unsafe(x);
[T | Float?(T) and not Flag?(#"FAST_MATH")]
inline overload exp(x:T) = exp_safe(x);
