
overload Bool() Bool {
}

overload Bool(src:Bool) returned:Bool {
    primitiveCopy(returned, src);
}

overload destroy(x:Bool) {
}

[I | Integer?(I)]
overload I(a:Bool) {
    if (a) return I(1);
    else return I(0);
}

[I | Integer?(I)]
overload Bool(a:I) = (a != I(0));

overload equals?(a:Bool, b:Bool) {
    if (a) return b;
    return not b;
}

overload lesser?(a:Bool, b:Bool) = ((not a) and b);

overload assign(dest:Bool, src:Bool) {
    primitiveCopy(dest, src);
}
