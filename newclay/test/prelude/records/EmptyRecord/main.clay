import unsafe.valuesemantics.(PODType?);
import show.*;

symbol Empty = RecordType();

main() {
    show(..#fieldNames(Empty));
    show(..#fieldTypes(Empty));

    show(#PODType?(Empty)); ////true
    show(#typeSize(Empty)); ////0u
    show(#typeAlign(Empty)); ////1u

    var n = Empty();

    show(..*n);
}
