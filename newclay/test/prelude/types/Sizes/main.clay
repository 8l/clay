import show.*;

main() {
    show(#TypeSize(Bool)); ////1
    show(#TypeSize(Int8)); ////1
    show(#TypeSize(Int16)); ////2
    show(#TypeSize(Int32)); ////4
    show(#TypeSize(Int64)); ////8
    show(#TypeSize(Float32)); ////4
    show(#TypeSize(Float64)); ////8
    show(#TypeSize(Char)); ////1
    show(#TypeSize(Char16)); ////2
    show(#TypeSize(Char32)); ////4
    // XXX platform dependent
    show(#TypeSize(OpaquePointer)); ////8
    show(#TypeSize(Tuple[Int8])); ////1
    show(#TypeSize(Tuple[Int8, Int8, Int8])); ////3
    show(#TypeSize(Tuple[Int8, Int16])); ////4
    show(#TypeSize(Tuple[Int8, Int32])); ////8
    show(#TypeSize(Tuple[Int32, Int8])); ////8
    show(#TypeSize(Tuple[Int32, Int8, Int64])); ////16
    show(#TypeSize(Union[Int8])); ////1
    show(#TypeSize(Union[Int8, Int16])); ////2
    show(#TypeSize(Union[Tuple[Int8, Int8, Int8], Int16])); ////4
    show(#TypeSize(Array[Int32, 16])); ////64
    show(#TypeSize(Array[Tuple[Int8, Int8, Int8], 16])); ////48
    // XXX platform dependent
    show(#TypeSize(StringConstant)); ////16
}
