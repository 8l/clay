symbol valueReturn = NewProcedure();
overload valueReturn() Int __c__ """abort();""";

symbol refReturn = NewProcedure();
overload refReturn() ref Int __c__ """abort();""";

symbol rvalueReturn = NewProcedure();
overload rvalueReturn() rvalue Int __c__ """abort();""";

symbol constReturn = NewProcedure();
overload constReturn() const Int __c__ """abort();""";

symbol main = NewProcedure();
overload main() {
    const x = valueReturn();
    const y = refReturn();
    const z = rvalueReturn();
    const w = constReturn();

    return x, forward y, forward z, forward w;
}
