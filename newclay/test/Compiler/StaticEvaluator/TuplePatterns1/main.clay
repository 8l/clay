import show.(show);
import __primitives__.*;
symbol Empty;
symbol One;
symbol Two;
symbol Many;
symbol FullHouse;

symbol MatchTuple;
overload #MatchTuple(()) = Empty;
overload #MatchTuple(('x,)) = One;
overload #MatchTuple(('x, 'y)) = Two;
overload #MatchTuple(('x, 'y, 'z, ..'w)) = Many;
overload #MatchTuple(('x, 'y, 'z, ..'w)) = Many;
overload #MatchTuple(('x, 'x, 'x, 'y, 'y)) = FullHouse;

#Main() =
    MatchTuple(()),
    MatchTuple((1,)),
    MatchTuple((1, true)),
    MatchTuple((1, true, '3')),
    MatchTuple((1, 1, 1, 2, 2));

main() { show(..#Main()); }
