import meta.symbols.*;
import show.*;

symbol Foo;
symbol Bar = Foo;
symbol Bas = Foo, Bar;

symbol Zim = Foo;
attribute Zim = Bar;

symbol Zang;
attribute Zang = Foo;
attribute Zang = Bar;
attribute Zang = Foo;

showSymbol(sym) {
    show(sym);
    show("--body");
    show(..#symbolBody(sym));
    show("--attributes");
    show(..#symbolAttributes(sym));
    show("");
}

main() {
    showSymbol(Foo);
    showSymbol(Bar);
    showSymbol(Bas);
    showSymbol(Zim);
    showSymbol(Zang);
}
