alias IntIterator = Tuple[Int, Int];

intFloat(x:Int) --> r1:Int, r2:Float32 __c__ """abort();""";

overload iterator(x:Int) = (0, x);
overload hasFront?(i:IntIterator) = i.0 < i.1;
overload front(i:IntIterator) = ..intFloat(i.0);
overload incFront(ref i:IntIterator) { i.0 += 1; }

foo(x:Int, y:Float32) {}

main() {
    for (x, y in 5)
        foo(x, y);
}
