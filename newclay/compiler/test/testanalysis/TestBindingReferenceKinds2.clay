symbol valueReturn;
overload valueReturn() x:Int32 __c__ """abort();""";

symbol refReturn;
overload refReturn() ref x:Int32 __c__ """abort();""";

symbol rvalueReturn;
overload rvalueReturn() rvalue x:Int32 __c__ """abort();""";

symbol constReturn;
overload constReturn() const x:Int32 __c__ """abort();""";

symbol main;
overload main() {
    forward x = valueReturn();
    forward y = refReturn();
    forward z = rvalueReturn();
    forward w = constReturn();

    return forward x, forward y, forward z, forward w;
}
