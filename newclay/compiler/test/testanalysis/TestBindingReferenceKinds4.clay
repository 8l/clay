symbol valueReturn;
overload valueReturn() x:Int32 __c__ """abort();""";

symbol refReturn;
overload refReturn() ref x:Int32 __c__ """abort();""";

symbol rvalueReturn;
overload rvalueReturn() rvalue x:Int32 __c__ """abort();""";

symbol constReturn;
overload constReturn() const x:Int32 __c__ """abort();""";

symbol main;
overload main() {
    var x = valueReturn();
    ref y = refReturn();
    rvalue z = rvalueReturn();
    const w = constReturn();

    return x, forward y, forward z, forward w;
}
