import __primitives__.*;

symbol foo;
overload #foo('x) | staticInt?('x) = 'x;
overload #foo('a') = 'a';
overload #foo('X') = staticError("I AM ERROR.");

#Main() =
    StaticExpressionDefined?(foo(1)),
    StaticExpressionDefined?(foo("a")),
    StaticExpressionDefined?(foo('a')),
    StaticExpressionDefined?(foo('X'));
