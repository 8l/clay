overload foo() {
    [y];
    [ref y];
    [-> y];
    [=> y];
    [x, y, ..z];
    [x += 1; x];
    [x += 1; y += 2; x, y];
    [x += 1;];

    [x -> y];
    [x => y];

    [x:Int -> y];
    [ref x -> y];
    [ref x:Int -> y];

    [x, y -> y, x];
    [ref x, rvalue y -> y, x];
    [ref x:Int, forward y:Bool -> y];
    [ref x:Int, forward ..y:'T -> ..y];

    [{ return x, y; }];
    [-> { return x, y; }];
    [=> { return x, y; }];
    [x, y -> {
        return x, y;
    }];
}
